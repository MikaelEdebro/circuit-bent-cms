<Project ToolsVersion="4.0" xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <PropertyGroup>
    <WebPublishMethod>MSDeploy</WebPublishMethod>
    <SiteUrlToLaunchAfterPublish>http://mamontswe.com</SiteUrlToLaunchAfterPublish>
    <MSDeployServiceURL>https://mikaeledebro-001-site8.smarterasp.net:8172/msdeploy.axd</MSDeployServiceURL>
    <DeployIisAppPath>mikaeledebro-001-site8</DeployIisAppPath>
    <RemoteSitePhysicalPath />
    <SkipExtraFilesOnServer>True</SkipExtraFilesOnServer>
    <MSDeployPublishMethod>WMSVC</MSDeployPublishMethod>
    <UserName>ifc\mikaeledebro-001</UserName>
    <_SavePWD>True</_SavePWD>
    <PublishDatabaseSettings>
      <Objects xmlns="">
        <ObjectGroup Name="DefaultConnection" Order="1" Enabled="False">
          <Destination Path="" />
          <Object Type="DbCodeFirst">
            <Source Path="DBContext" DbContext="CircuitBentCMS.Models.UsersContext, CircuitBentCMS" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="CircuitBentCMSContext" Order="2" Enabled="True">
          <Destination Path="Data Source=|DataDirectory|\CircuitBentCMS.sdf" />
          <Object Type="DbCodeFirst">
            <Source Path="DBMigration" DbContext="CircuitBentCMS.Models.CircuitBentCMSContext, CircuitBentCMS" MigrationConfiguration="CircuitBentCMS.Migrations.Configuration, CircuitBentCMS" Origin="Configuration" />
          </Object>
        </ObjectGroup>
        <ObjectGroup Name="elmah-sqlservercompact" Order="3" Enabled="False">
          <Destination Path="" />
          <Object Type="DbDacFx">
            <PreSource Path="Data Source=|DataDirectory|\Elmah.sdf" includeData="False" />
            <Source Path="$(IntermediateOutputPath)AutoScripts\elmah-sqlservercompact_IncrementalSchemaOnly.dacpac" dacpacAction="Deploy" />
          </Object>
          <UpdateFrom Type="Web.Config">
            <Source MatchValue="Data Source=|DataDirectory|\Elmah.sdf" MatchAttributes="$(UpdateFromConnectionStringAttributes)" />
          </UpdateFrom>
        </ObjectGroup>
      </Objects>
    </PublishDatabaseSettings>
  </PropertyGroup>
  <ItemGroup>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)CircuitBentCMSContext-Web.config Connection String">
      <ParameterValue>Data Source=|DataDirectory|\CircuitBentCMS.sdf</ParameterValue>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)DefaultConnection-Web.config Connection String">
      <UpdateDestWebConfig>False</UpdateDestWebConfig>
    </MSDeployParameterValue>
    <MSDeployParameterValue Include="$(DeployParameterPrefix)elmah-sqlservercompact-Web.config Connection String">
      <UpdateDestWebConfig>False</UpdateDestWebConfig>
    </MSDeployParameterValue>
  </ItemGroup>
</Project>